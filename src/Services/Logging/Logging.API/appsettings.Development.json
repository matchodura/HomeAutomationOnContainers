{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "SeqServerUrl": "http://192.168.1.181:5341/",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "EventTypeEnricher" ]
  },
  "AllowedHosts": "*",
  "BrokerHostSettings": {
    "Host": "192.168.1.181",
    "Port": 1883
  },

  "ClientSettings": {
    "Id": "test-client",
    "UserName": "dotnet-app",
    "Password": "dotnet-app"
  }
}

