{
  "DeviceDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "Devices",
    "DeviceCollectionName": "Device"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "SeqServerUrl": "http://192.168.0.183:5341/",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "EventTypeEnricher" ]
  },
  "AllowedHosts": "*",
  "BrokerHostSettings": {
    "Host": "192.168.0.183",
    "Port": 1883
  },
  //TODO remember to add the client to mosquitto
  "ClientSettings": {
    "Id": "status-client",
    "UserName": "status-app",
    "Password": "status-app"
  },
  "RabbitMQHost": "192.168.0.183",
  "RabbitMQPort": "5672"
}
