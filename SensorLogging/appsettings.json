{
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341/",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "EventTypeEnricher" ],
    "Properties": {
      "Application": "SensorLogging"
    }
  },
  "AllowedHosts": "*"

}